<!doctype html>
<html lang="<%= _.lang.replace('_', '-') %>" dir="ltr">

<head>
  <%- await include('@/head') %>
</head>

<body
  id="page-<%= _.pageName %>"
  class="_<%= _.route.path.replace(/\//g, '_') %>"
  <% if (_.route.resource) { %>
    data-resource="<%= _.route.resource %>"
    data-resource-id="<%= _.state._id %>"
  <% } else if (_.route.collection) { %>
    data-cms-collection="<%= _.route.collection %>"
  <% } %>
>
  <%- await include('@/layout/menu') %>

  <main role="main" id="main">
    <%- await include('@/layout/header') %>
    
    <div class="content-orcamento">
      <div class="content-title-page"><h1 class="title-page" style="border-bottom: 0;">Obrigado por falar conosco, seu orçamento foi enviado com sucesso.</h1></div>
      <p class="text-send">Recebemos sua solicitação de orçamento, em breve entraremos em contato com você.</p>
      <a href="/" class="button-return">Continuar navegando.</a>
      <div class="container">
        
      </div>
    </div>

    <%- await include('@/layout/footer') %>
  </main>

  <%- await include('@/json') %>
  <%- await include('@/scripts') %>
</body>

</html>